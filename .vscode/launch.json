{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Spring Boot Application",
            "type": "java",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "mainClass": "com.microsoft.ganesha.GaneshaApplication",
            "args": [
                "spring-boot:run"
            ],
            "serverReadyAction": {
                "pattern": "Tomcat initialized with port ([0-9]+)",
                "uriFormat": "http://localhost:%s/index.html",
                "action": "openExternally"
            }
        }
    ]
}